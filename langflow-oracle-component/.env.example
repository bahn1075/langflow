# Oracle Database Configuration
# Copy this file to .env and fill in your actual credentials

# Oracle Database Credentials
ORACLE_DB_USER=ADMIN
ORACLE_DB_PASSWORD=YourPasswordHere
ORACLE_DSN=myatp_high

# Oracle Wallet Configuration
ORACLE_WALLET_DIR=/path/to/wallet
ORACLE_WALLET_PASSWORD=YourWalletPasswordHere

# Vector Store Configuration
ORACLE_TABLE_NAME=PDFCOLLECTION

# Embedding Model Configuration
EMBEDDING_MODEL=sentence-transformers/all-MiniLM-L6-v2
EMBEDDING_DIMENSION=384

# Search Configuration (Optional)
DEFAULT_K=5
DEFAULT_FETCH_K=20
DEFAULT_SCORE_THRESHOLD=0.35
DEFAULT_DISTANCE_STRATEGY=COSINE

# Langflow Configuration (Optional)
LANGFLOW_HOST=127.0.0.1
LANGFLOW_PORT=7860
